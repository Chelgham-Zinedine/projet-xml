    <!ELEMENT adresse (#PCDATA)>
    <!ELEMENT telephone (#PCDATA)>
    <!ELEMENT mail (#PCDATA)>
    <!ELEMENT site (#PCDATA)>
    <!ELEMENT description (#PCDATA)>
    <!ELEMENT photo (#PCDATA)>
    <!ENTITY % offres_sejours SYSTEM "offres_sejours.dtd">%offres_sejours;


]>
